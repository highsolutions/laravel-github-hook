{
    "name": "highsolutions/github-hook",
    "description": "A Laravel package for simple Continuous Integration based on GitHub push.",
    "keywords": ["laravel", "github", "continuous", "integration", "ci", "git", "hook"],
    "authors": [
        {
            "name": "HighSolutions",
            "email": "adam@highsolutions.pl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x"
    },
    "autoload": {
        "psr-4": {
            "HighSolutions\\GitHubHook\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5.6"],
        "laravel": {
            "providers": [
                "HighSolutions\\GitHubHook\\GitHubHookServiceProvider"
            ]            
        }
    },
    "minimum-stability": "stable"
}
